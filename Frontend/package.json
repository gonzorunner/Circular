{
  "name": "cse186-assignment8-frontend",
  "version": "2.0.0",
  "description": "CSE186 Assignment 8 - Frontend",
  "author": "David Harrison <dcharris@ucsc.edu>",
  "license": "UNLICENSED",
  "repository": "none",
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "@react-google-maps/api": "^2.18.1",
    "@react-oauth/google": "^0.7.3",
    "axios": "^1.3.3",
    "emoji-picker-react": "*",
    "google-auth-library": "^8.7.0",
    "google-maps-react": "^2.0.6",
    "latest-version": "^7.0.0",
    "prop-types": "*",
    "react": "*",
    "react-dom": "^18.0.0",
    "react-geocode": "^0.2.3",
    "react-google-autocomplete": "^2.7.3",
    "react-google-maps": "^9.4.5",
    "react-router": "*",
    "react-router-dom": "*",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "bestzip": "*",
    "eslint-config-google": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "msw": "*",
    "puppeteer": "^19.6.3",
    "puppeteer-core": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test --coverage --verbose",
    "lint": "eslint ./src/**/*.jsx ./src/**/*.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "indent": [
        "warn",
        2
      ],
      "linebreak-style": 0,
      "require-jsdoc": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
