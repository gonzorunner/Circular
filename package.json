  "bugs": {
    "url": "https://github.com/IanC27/circular-trade-app/issues"
  },
  "homepage": "https://github.com/IanC27/circular-trade-app#readme",
  "dependencies": {
    "react-google-autocomplete": "^2.7.3"
  }
}
